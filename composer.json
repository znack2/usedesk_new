{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "dingo/api": "2.0.0-alpha1",
        "domnikl/statsd": "~2.0",
        "dotzero/amocrm": "^0.3.26",
        "elasticquent/elasticquent": "^1.0",
        "fideloper/proxy": "~3.3",
        "jenssegers/mongodb": "^3.3",
        "laravel-shield/shield": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravel/tinker": "~1.0",        
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^5.0.0",
        "spatie/laravel-failed-job-monitor": "^3.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^5.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "mpociot/laravel-test-factory-helper": "^0.5.0",
        "nunomaduro/collision": "^1.1",
        "phpunit/phpunit": "~6.0",
        "pragmarx/ci": "^0.9.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
